{
    "bert_model_name": "bert-large-cased",
    "bert_cache_dir": "./bert",
    "use_bert": false,
    "use_char_emb": true,
    "multi_piece_strategy": "average",
    "bert_dropout": 0.5,
    "linear_dropout": 0.4,
    "linear_bias": true,
    "linear_activation": "relu",
    "entity_hidden_num": 200,
    "train_file": "../GenSP/processed_data/conll03_t5/train.json",
